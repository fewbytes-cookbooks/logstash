input {
	tcp {
		port => "<%= node["logstash"]["config"]["tcp"]["port"] %>"
		mode => "server"
	}

	udp {
		port => "<%= node["logstash"]["config"]["udp"]["port"] %>"
	}
}

output {
	elasticsearch {
<% node["logstash"]["config"]["elasticsearch"].each do |k, v| -%>
		<%= k %> => <%= v.inspect %>
<% end -%>
	}
}